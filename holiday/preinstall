SELECT 'BEF', SQL.ENV, COUNT(*) FROM USERTMTBL 
FORMAT ADDTO '../../system/upgrades/USERTMTBL-COUNT.TXT';
SELECT * FROM USERTMTBL 
DATA STRCAT('../../system/upgrades/USERTMTBL',SQL.ENV,'.DAT');
