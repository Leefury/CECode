/*
Skip - no change */
GOTO 99 WHERE :$.COL = :$1.COL ;
UPDATE ZCLA_ROOMS
SET    COL = :$.COL
WHERE  0=0
AND    HOUSETYPEID = :$.HOUSETYPEID ;
UPDATE ZCLA_COMPONENT
SET    COL = :$.COL
WHERE  0=0
AND    ROOM IN (
SELECT ROOM FROM ZCLA_ROOMS
WHERE  0=0
AND    HOUSETYPEID = :$.HOUSETYPEID
);
LABEL 99 ;