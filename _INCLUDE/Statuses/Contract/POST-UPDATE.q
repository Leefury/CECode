SELECT PROJACTS.PROJACT
FROM   ZCLA_CONTRACTEL , ZCLA_CONTRACTS , PROJACTS , ZCLA_ELACTSTAT
WHERE 0=0
AND   ZCLA_ELACTSTAT.PROJACT = PROJACTS.PROJACT
