/* Copy FROM site TO site
*/
#INCLUDE func/ZCLA_DEBUGUSR
SELECT 'ZCLA_COPYHOUSE/170' FROM DUMMY
WHERE :DEBUG = 1 FORMAT ADDTO :DEBUGFILE 
;
:DOC = 0 ;
:DOCNO = '' ;
SELECT DOC , DOCNO INTO :DOC , :DOCNO
FROM DOCUMENTS
WHERE 0=0
AND   DOC = :$.DOC
AND   TYPE = 'p'
;
/* Check exists
*/
SELECT :$.NA3 , :DOCNO
INTO :PAR1, :PAR2
FROM DUMMY ;
ERRMSG 800 WHERE EXISTS (
SELECT 'x'
FROM ZCLA_HOUSETYPE
WHERE 0=0
AND   DOC = :DOC
AND   TYPENAME = :$.NA3
);
SELECT :DOC , :$.HS
FROM DUMMY
FORMAT ADDTO :DEBUGFILE
;
SELECT SQL.TMPFILE
INTO :GEN FROM DUMMY;
LINK GENERALLOAD TO :GEN;
ERRMSG 1 WHERE :RETVAL = 0
;
/*
