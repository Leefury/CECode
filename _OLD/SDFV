SELECT ( SUM(ZCLA_SITEELFIX.SPLIT) = 100 ? 'Y' : 'N' )
INTO   :$.SITESPLIT
FROM  ZCLA_SITEELFIX ?
,     PROJACTS ELACT 
,     ZCLA_HOUSETYPE
,     ZCLA_ELEMENTFIX
WHERE 0=0
AND ELACT.ZCLA_HOUSETYPEID = ZCLA_HOUSETYPE.HOUSETYPEID 
AND ZCLA_HOUSETYPE.EL = ZCLA_ELEMENTFIX.EL 
AND ZCLA_SITEELFIX.EL = ZCLA_ELEMENTFIX.EL 
AND ZCLA_SITEELFIX.FIXID = ZCLA_ELEMENTFIX.FIXID                         
GROUP BY ELACT.PROJACT
HAVING ELACT.PROJACT = 653