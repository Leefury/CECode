:CONTRACT = 0 ;
SELECT ZCLA_CONTRACTS.CONTRACT INTO :CONTRACT
FROM   ZCLA_CONTRACTSTATUSE ,
       PROJACTS ,
       ZCLA_CONTRACTEL ,
       ZCLA_CONTRACTS 
WHERE 0=0
AND   PROJACTS.ZCLA_EL = ZCLA_CONTRACTEL.EL 
AND   ZCLA_CONTRACTEL.CONTRACT = ZCLA_CONTRACTS.CONTRACT 
AND   PROJACTS.DOC = ZCLA_CONTRACTS.DOC 
AND   ZCLA_CONTRACTSTATUSE.STEPSTATUS = ZCLA_CONTRACTS.STEPSTATUS
AND   PROJACTS.PROJACT = 335
;