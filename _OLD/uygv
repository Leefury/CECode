SELECT ZCLA_CONTRACTSTATUSE.STATLOCK
FROM   ZCLA_CONTRACTS
,      ZCLA_CONTRACTEL 
,      ZCLA_CONTRACTSTATUSE 
,      PROJACTS 
WHERE 0 = 0
AND   ZCLA_CONTRACTS.CONTRACT = ZCLA_CONTRACTEL.CONTRACT
AND   ZCLA_CONTRACTS.STEPSTATUS = ZCLA_CONTRACTSTATUSE.STEPSTATUS
AND   ZCLA_CONTRACTEL.EL = PROJACTS.ZCLA_EL AND ZCLA_CONTRACTS.DOC = PROJACTS.DOC
AND   PROJACTS.PROJACT = 854