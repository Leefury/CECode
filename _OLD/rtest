SELECT  CUSTNOTESA.SUBJECT, USERS1.USERLOGIN, CUSTNOTES.CURDATE,
 STRIND(DTOA(CUSTNOTES.CURDATE,'day'),1,3), CUSTNOTES.STIME, CUSTOMERS.CUSTNAME,
 CUSTOMERS.CUSTDES, CUSTTOPICS.TOPICCODE, CUSTTOPICS.TOPICDES,
 CUSTNOTETYPES.CUSTNOTETYPEDES, CAMPAIGNS.CAMPAIGNCODE, CAMPAIGNS.CAMPAIGNDES,
 CUSTCAMPAIGNS.CCNUM, CUSTNOTESA.TILLDATE, STRIND(DTOA(CUSTNOTESA.TILLDATE,'day'),1,3),
 CUSTNOTES.ETIME, CUSTNOTESTATUSES.STATDES, CUSTNOTES.CLOSED,
 CUSTNOTES.STATUSDATE, CUSTNOTESA.CLOSEDATE, CUSTNOTES.PRIO, CUSTNOTES.CUSTNOTE,
 PHONEBOOK.NAME, 00:00 + ROUND(CUSTNOTESA.PLANNEDHOURS * 60),
 PHONEBOOK.PHONENUM, PHONEBOOK.CELLPHONE, PHONEBOOK.EMAIL, CUSTNOTESA.LOCATION,
 CTYPE.CTYPECODE, CTYPE.CTYPENAME, DESTCODES.CODE, DESTCODES.CODEDES,
 CUSTOMERS.PHONE, SUPPLIERS.SUPNAME, SUPPLIERS.SUPDES, SUPPLIERS.PHONE,
 PHONEBOOK2.NAME, PHONEBOOK2.PHONENUM, PHONEBOOK2.CELLPHONE, USERS.USERLOGIN,
 BRANCHES.BRANCHNAME, BRANCHES.BRANCHDES, (SQL.LANGUAGE = SYSCONST.VALUE 
OR (SQL.LANGUAGE = 2 AND SYSCONST.VALUE = 3) OR LANGTRIGMSG.MESSAGE = '' 
? STRPREFIX(TRIGMSG.MESSAGE,20) : STRPREFIX(LANGTRIGMSG.MESSAGE,20)),
 CUSTNOTES.DOCNO, DOCUMENTS1.DOCNO, DOCPROJ.PROJDES, PROJACTS.WBS,
 CUSTNOTESA.PROJACT, PROJACTS.ACTDES, CUSTOMERS.ADDRESS, CUSTOMERS.STATE,
 AGENTS.AGENTCODE, AGENTS.AGENTNAME, CUSTNOTES.CUSER, SHIPTO.LAT,
 SHIPTO.LONG, CUSTNOTES.OUDATE, CUSTNOTES.CUDATE, CUSTNOTESA.REMINDFLAG,
 CUSTNOTESA.REMINDTIME, USERS4.USERLOGIN, USERS5.USERLOGIN, USERS6.USERLOGIN,
 USERS7.USERLOGIN, USERS8.USERLOGIN, UGROUPS.GROUPNAME, UGROUPS.GROUPDES,
 UGROUPS2.GROUPNAME, UGROUPS2.GROUPDES, UGROUPS3.GROUPNAME, UGROUPS3.GROUPDES,
 CUSTNOTESA.EXTFILEFLAG, RESOURCES.RESOURCENAME, RESOURCES2.RESOURCENAME,
 RESOURCES3.RESOURCENAME, CUSTNOTESA.ORIGDATE, CUSTNOTESA.REMARK,
 CUSTNOTESA.PREVCUSTNOTE, USERS3.USERLOGIN, QUESTFORM.QUESTFCODE,
 QUESTFORM.QUESTFDES, QUESTFORM.SOF_SIGN, CUSTNOTESA.UDATE, USERS2.USERLOGIN,
 CUSTNOTES.ZGEM_EVENTID, PROJACTS8.PROJACT, PROJACTS9.PROJACT,
 CUSTNOTES.ZCLA_PASSMARK, ZCLA_FIXSTATUSES.STEPSTATUSDES, CUSTNOTES.ZGCW_PANELNUMBER,
 CUSTNOTES.ZGEM_ANSEXTRACTED, CUSTNOTES.ZGEM_AUDIT, CUSTNOTES.ZGEM_EXTFILENAME,
 CUSTNOTES.ZGEM_INTINVSENT, CUSTNOTES.ZGEM_INTINVSTAMP, ORDERS9.ORDNAME,
 CUSTNOTES.ZGEM_PLOT, QUESTFORM.QUESTFCODE, CUSTNOTES.ZGEM_REGNUM,
 CUSTNOTES.ZGEM_SCORE, DOCUMENTS9.DOCNO, CUSTNOTES.ZGEM_SITEDES,
 USERSB.SNAME, DOCUMENTS1.ZCLA_PHASE, CUSTNOTES.CUSTNOTE, CUSTNOTES.SOF_QFINAL,
 PROJACTS.DOC, ZGEM_QUESTTYPES.QUESTTYPEDES, STRIND(DTOA(CUSTNOTES.CURDATE,'day'),1,3),
 CUSTNOTESA.PLANNEDHOURS, DOCUMENTS.DOCNO, DOCTYPES.DOCDES, CUSTNOTESA.PROJACT,
 ORDERS.ORDNAME, CUSTOMERS.FAX, CUSTNOTESA.AGENT, CUSTNOTESA.AUTOFLAG,
 CUSTOMERSA.BILLINGUSER, CAMPAIGNS.CAMPAIGN, CUSTNOTESTATUSES.CANCELFLAG,
 CUSTNOTESA.CC, CUSTNOTESTATUSES.CHLFLAG, CUSTNOTESTATUSES.CLOSEDFLAG,
 CUSTNOTES.COLOR, CUSTNOTES.CUST, CUSTOMERS.CUST, CUSTNOTES.CUSTNOTESTATUS,
 CUSTNOTESA.CUSTNOTESTATUS, CUSTNOTES.CUSTNOTETYPE, PROJACTS.DOC,
 DOCUMENTS1.DOC, CUSTNOTESA.DOC, CUSTNOTES.DOC, DOCUMENTS1.DOC,
 PHONEBOOK.FIRM, CUSTNOTES.CUSTNOTE, FOLLOWUPLIST.TYPE, FOLLOWUPLIST.USER,
 0, '', 0, 0, DOCPROJ.MUSER, PROJACTS.NONSTANDARD, ORDERS.ORD,
 CUSTNOTES.OUSER, CUSTNOTES.PHONE, PHONEBOOK.POSITION, CUSTNOTES.PROF,
 DOCUMENTS1.CUST, DOCUMENTS.RMADOC, CUSTNOTESTATUSES.CHANGEFLAG,
 'n', CUSTNOTESA.SUP, CUSTNOTESA.SUPPHONEB, CUSTNOTESA.USER, CUSTNOTES.TOPIC,
 DOCUMENTS.TYPE, SHIPTO.TYPE, CUSTNOTES.CUSER, DOCTYPES.EXEC,
 CUSTNOTESA.CUSER2, CUSTNOTESA.CUSER3, CUSTNOTESA.CUSER4, CUSTNOTESA.CUSER5,
 CUSTNOTESA.CUSER6, CUSTNOTESA.CUSER7, CUSTNOTESA.CUSER8, CUSTNOTESA.UGROUP,
 CUSTNOTESA.UGROUP2, CUSTNOTESA.UGROUP3, CUSTTOPICS.MILESTONE,
 CUSTNOTESA.RESOURCE, CUSTNOTESA.RESOURCE2, CUSTNOTESA.RESOURCE3,
 'N', CUSTNOTES.CUSTNOTE, 0, CUSTNOTESB.CLID, CUSTOMERS.CTYPE,
 CUSTNOTESB.DESTCODE, CUSTTOPICS.MAILFLAG, '\0', CUSTOMERS.CURRENCY,
 DOCPROJ.ROADTIME, DOCPROJ.ROADTIMEI, CUSTOMERS.MCUST, DOCUMENTS.PL,
 DOCUMENTS.PLIST, QUESTFORM.PRTSIGN, CUSTNOTESA.PRTQFINAL, DOCUMENTS.PARENTSERN,
 '', CUSTNOTES.TYPE, CUSTNOTES.IV, TRIGMSG.EXEC, LANGTRIGMSG.LANG,
 LANGTRIGMSG.EXEC, LANGTRIGMSG.NUM, SYSCONST.NAME, CUSTNOTES.DOCNO,
 TRIGMSG.NUM, -1, '', '', CUSTNOTES.BRANCH, PHONEBOOK.EMAIL, CUSTNOTES.CUSTNOTE,
 CUSTOMERS.SECONDLANGTEXT, CUSTOMERS.AGENT, SUPPLIERS.SECONDLANGTEXT,
 SEARCHDOCVERS.FROMEDI, CUSTNOTES.CUSTNOTE, SEARCHDOCVERS.STATUSTYPE,
 'Y', '\0', '\0', CUSTNOTESB.QUESTF, QUESTFORM.TYPE, CUSTNOTESA.USER,
 PROJACTS.ZCLA_EL, PROJACTS.ZCLA_PLOT, PROJACTS.STEPSTATUS, CUSTNOTES.ZGEM_CUSTRETURNDOC,
 CUSTNOTES.ZGEM_USERB, CUSTNOTES.ZGEM_ORD, /*This Field is needed to extract 
asnwers from audits*/ '', CUSTNOTES.ZGEM_SHIPPINGDOC, /*This Field is needed 
to extract asnwers from audits*/ '', DOCUMENTS9.TYPE, CUSTNOTES.ZCLA_CONTRACTSCHED,
 CUSTNOTES.CUSTNOTETYPE, CUSTNOTES.ZCLA_WK, CUSTNOTES.ZCLA_YR,
 QUESTFORM.ZGEM_QUESTTYPE
  FROM  STATUSTYPES !, CPROF  , POSITIONS  , NONSTANDARD ?, SEARCHDOCVERS 
?, SYSCONST  , LANGTRIGMSG  , TRIGMSG  , CUSTNOTESB ?, FOLLOWUPLIST ?,
 CAMPAIGNS ?, CUSTOMERSA  , ORDERS  , DOCTYPES  , DOCUMENTS  ,
 ZGEM_QUESTTYPES ?, USERSB  , DOCUMENTS DOCUMENTS9 ?, ORDERS ORDERS9 ?,
 ZCLA_FIXSTATUSES ?, PROJACTS PROJACTS9 ?, PROJACTS PROJACTS8 ?,
 USERS USERS2  , QUESTFORM ?, USERS USERS3  , RESOURCES RESOURCES3  ,
 RESOURCES RESOURCES2  , RESOURCES  , UGROUPS UGROUPS3  , UGROUPS UGROUPS2 
 , UGROUPS  , USERS USERS8  , USERS USERS7  , USERS USERS6  ,
 USERS USERS5  , USERS USERS4  , SHIPTO ?, AGENTS !, PROJACTS ?,
 CUSTNOTESA !, DOCPROJ  , DOCUMENTS DOCUMENTS1  , BRANCHES  ,
 USERS  , PHONEBOOK PHONEBOOK2  , SUPPLIERS  , DESTCODES ?, CTYPE  ,
 PHONEBOOK  , CUSTNOTESTATUSES !, CUSTCAMPAIGNS  , CUSTNOTETYPES !,
 CUSTTOPICS !, CUSTOMERS !, CUSTNOTES !, USERS USERS1 ! 
WHERE  (CUSTNOTES.CURDATE   <> 0)
AND  CUSTNOTES.CUSTNOTE = CUSTNOTESA.CUSTNOTE
AND  CUSTNOTESA.PROJACT = PROJACTS.PROJACT
AND  (CUSTNOTES.CUSER   >= 0 OR CUSTNOTES.CUSER = CUSTNOTES.CUSER)

AND  CUSTNOTESA.AGENT = AGENTS.AGENT
AND  CUSTOMERSA.BILLINGUSER = USERS3.USER
AND  (CAMPAIGNS.CAMPAIGN   = CUSTNOTESB.CAMPAIGN)
AND  CUSTNOTESA.CC = CUSTCAMPAIGNS.CC
AND  CUSTNOTES.CUST = CUSTOMERS.CUST
AND  CUSTOMERS.CUST = CUSTOMERSA.CUST
AND  CUSTNOTES.CUSTNOTESTATUS = CUSTNOTESTATUSES.CUSTNOTESTATUS
AND  CUSTNOTES.CUSTNOTETYPE = CUSTNOTETYPES.CUSTNOTETYPE
AND  (PROJACTS.DOC   = (CUSTNOTES.TYPE IN ('Q','H') ? CUSTNOTES.IV : 0))
AND  DOCUMENTS1.DOC = DOCPROJ.DOC
AND  CUSTNOTESA.DOC = DOCUMENTS1.DOC
AND  DOCUMENTS1.DOC = SHIPTO.IV
AND  CUSTNOTES.CUSTNOTE = FOLLOWUPLIST.IV
AND  (FOLLOWUPLIST.TYPE = 'n')
AND  (FOLLOWUPLIST.USER = 0 + SQL.USER)
AND  PROJACTS.NONSTANDARD = NONSTANDARD.NONSTANDARD
AND  (ORDERS.ORD   = (CUSTNOTES.TYPE = 'O' ? CUSTNOTES.IV : 0))
AND  CUSTNOTES.OUSER = USERS.USER
AND  CUSTNOTES.PHONE = PHONEBOOK.PHONE
AND  PHONEBOOK.POSITION = POSITIONS.POSITION
AND  CUSTNOTES.PROF = CPROF.PROF
AND  CUSTNOTESA.SUP = SUPPLIERS.SUP
AND  CUSTNOTESA.SUPPHONEB = PHONEBOOK2.PHONE
AND  CUSTNOTES.TOPIC = CUSTTOPICS.TOPIC
AND  DOCUMENTS.TYPE = DOCTYPES.TYPE
AND  (SHIPTO.TYPE   ='D')
AND  CUSTNOTES.CUSER = USERS1.USER
AND  CUSTNOTESA.CUSER2 = USERS4.USER
AND  CUSTNOTESA.CUSER3 = USERS5.USER
AND  CUSTNOTESA.CUSER4 = USERS6.USER
AND  CUSTNOTESA.CUSER5 = USERS7.USER
AND  CUSTNOTESA.CUSER6 = USERS8.USER
AND  CUSTNOTESA.UGROUP = UGROUPS.UGROUP
AND  CUSTNOTESA.UGROUP2 = UGROUPS2.UGROUP
AND  CUSTNOTESA.UGROUP3 = UGROUPS3.UGROUP
AND  CUSTNOTESA.RESOURCE = RESOURCES.RESOURCE
AND  CUSTNOTESA.RESOURCE2 = RESOURCES2.RESOURCE
AND  CUSTNOTESA.RESOURCE3 = RESOURCES3.RESOURCE
AND  CUSTOMERS.CTYPE = CTYPE.CTYPE
AND  CUSTNOTESB.DESTCODE = DESTCODES.DESTCODE
AND  CUSTNOTES.TYPE = STATUSTYPES.TYPE
AND  (TRIGMSG.EXEC   = (STATUSTYPES.TYPE = '' ? 0 : STATUSTYPES.DOCEXEC))
AND  (LANGTRIGMSG.LANG   = (STATUSTYPES.TYPE = '' OR SQL.LANGUAGE = SYSCONST.VALUE
? 0 : SQL.LANGUAGE))
AND  (LANGTRIGMSG.EXEC   = (STATUSTYPES.TYPE = '' OR SQL.LANGUAGE = SYSCONST.VALUE
? 0 : TRIGMSG.EXEC))
AND  (LANGTRIGMSG.NUM   = (STATUSTYPES.TYPE = '' OR SQL.LANGUAGE = SYSCONST.VALUE
? 0 : 402))
AND  (SYSCONST.NAME   = 'LANGUAGE')
AND  (TRIGMSG.NUM   = (STATUSTYPES.TYPE = '' ? 0 : 402))
AND  CUSTNOTES.BRANCH = BRANCHES.BRANCH
AND  CUSTNOTES.CUSTNOTE = CUSTNOTESB.CUSTNOTE
AND  CUSTNOTES.CUSTNOTE = SEARCHDOCVERS.IV
AND  (SEARCHDOCVERS.STATUSTYPE   = 'n')
AND  CUSTNOTESB.QUESTF = QUESTFORM.QUESTF
AND  CUSTNOTESA.USER = USERS2.USER
AND  PROJACTS.ZCLA_EL = PROJACTS8.PROJACT
AND  PROJACTS.ZCLA_PLOT = PROJACTS9.PROJACT
AND  PROJACTS.STEPSTATUS = ZCLA_FIXSTATUSES.STEPSTATUS
AND  CUSTNOTES.ZGEM_USERB = USERSB.USERB
AND  CUSTNOTES.ZGEM_ORD = ORDERS9.ORD
AND  CUSTNOTES.ZGEM_SHIPPINGDOC = DOCUMENTS9.DOC
AND  (CUSTNOTES.CUSTNOTETYPE   = 7)
AND  QUESTFORM.ZGEM_QUESTTYPE = ZGEM_QUESTTYPES.QUESTTYPE 
 ORDER BY 3 ASC ,5 ASC ,76 ASC ,22 ASC 